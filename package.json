    "file-loader": "^5.0.2",
    "gh-pages": "^2.0.1",
    "grunt": "^1.0.4",
    "grunt-banner": "^0.6.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-less": "^2.0.0",
    "grunt-contrib-pug": "^2.0.0",
    "grunt-he": "^0.1.0",
    "grunt-uncss": "^0.9.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-emit-webpack-plugin": "^2.0.2",
    "json-loader": "^0.5.7",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "load-grunt-config": "^3.0.1",
