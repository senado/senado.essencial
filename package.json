    "standard": "^14.3.1",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "superstatic": "^6.0.4",
    "terser-webpack-plugin": "^2.3.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.10.1",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "jquery": "^3.5.0",
    "scriptjs": "^2.5.9",
    "senado.css": "git+https://github.com/fabianonunes/senado.css.git#2.11.0"
